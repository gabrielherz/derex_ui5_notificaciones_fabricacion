PROCESS BEFORE OUTPUT.

MODULE %_INIT_PBO.

MODULE %_PBO_REPORT.

MODULE %_PF_STATUS.

MODULE %_R_PROV.

MODULE %_R_MATNR.

MODULE %_R_FEC_EN.

MODULE %_R_BEDAT.

MODULE %_R_EINDT.

MODULE %_R_EBELN.

MODULE %_R_EBTYP.

MODULE %_END_OF_PBO.

PROCESS AFTER INPUT.

  MODULE %_BACK AT EXIT-COMMAND.

  MODULE %_INIT_PAI.

